            <FilterWorkBg order={3}>\n                <FilterWork\n                  onClick={() => setDiscipline(\"dev\")}\n                  onMouseEnter={() => setBlur(true)}\n                  onMouseLeave={() => setBlur(false)}\n                >\n                  Developer\n                </FilterWork>\n              </FilterWorkBg>\n            </FlexWrapper>\n          </Col>\n          <Col width={[1, 1 / 2, 7 / 12]}>\n            {!discipline ? (\n              <CuteMessageWrap>\n                <h3>What would you like to see?</h3>\n              </CuteMessageWrap>\n            ) : viewer ? (\n              <>\n                <FlexWrapper justify=\"space-between\">\n                  <h2 style={{ marginBottom: `0px` }}>{focusClient}</h2>\n                  <Close\n                    style={{ width: `36px`, height: `36px`, cursor: `pointer` }}\n                    onClick={() => setViewer(false)}\n                  />\n                </FlexWrapper>\n                <ViewContainer width={clients.length}>\n                  <div className=\"viewInner\">\n                    {clients.map((client) => (\n                      <ImgWrapper\n                        onMouseEnter={() => setFocusClient(client.clientName)}\n                      >\n                        <Image\n                          src={client.imageThumb}\n                          alt=\"Picture of the author\"\n                          layout=\"fill\"\n                        />\n                      </ImgWrapper>\n                    ))}\n                  </div>\n                </ViewContainer>\n              </>\n            ) : (\n              <GridWrap wrap={true} justify=\"flex-start\">\n                {clients.map((client) => (\n                  <ClientThumbWrap\n                    key={client.id}\n                    onClick={() => setViewer(true)}\n                    display={\n                      ((client.ux && discipline === \"ux\") ||\n                        (client.