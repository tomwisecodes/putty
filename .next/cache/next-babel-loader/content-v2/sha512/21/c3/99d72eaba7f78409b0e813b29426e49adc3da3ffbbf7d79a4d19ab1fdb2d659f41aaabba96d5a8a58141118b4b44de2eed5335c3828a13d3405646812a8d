      }\n    }\n  }, [chaos]);\n\n  useEffect(() => {\n    const setFromEvent = (e) => {\n      setMouseX(e.pageX);\n      setMouseY(e.pageY);\n    };\n    const mouseMoving = () => {\n      setMouseMove(true);\n    };\n\n    window.addEventListener(\"mousemove\", setFromEvent);\n    window.addEventListener(\"mousemove\", mouseMoving);\n\n    return () => {\n      window.removeEventListener(\"mousemove\", setFromEvent);\n    };\n  }, []);\n\n  useEffect(() => {\n    cricleRef.current.style.transform = `translate3d( ${mouseX}px, ${mouseY}px, 0) `;\n  }, [mouseX]);\n\n  function startOrReset() {\n    if (choas) {\n      kickOffTransition();\n    } else {\n      for (const i = 0; i < theThings.length; i++) {\n        const theThing = theThings[i];\n\n        theThing.removeEventListener(\"transitionend\", updatePosition, false);\n        theThing.removeEventListener(\n          \"webkitTransitionEnd\",\n          updatePosition,\n          false\n        );\n        theThing.removeEventListener(\"mozTransitionEnd\", updatePosition, false);\n        theThing.removeEventListener(\"msTransitionEnd\", updatePosition, false);\n        theThing.removeEventListener(\"oTransitionEnd\", updatePosition, false);\n\n        setTranslate3DTransform(theThing);\n        setTransitionDuration(theThing);\n      }\n    }\n  }\n\n  return (\n    <>\n      <Header />\n      {mouseMove ? \"yadsd\" : \"noasdf\"}\n      <br />\n      <p ref={choasRef} className=\"test\">\n        {chaos ? \"this is chaos\" : \"peace time\"}\n      </p>\n      <p ref={choasRef} className=\"test\">\n        {chaos ? \"this is chaos\" : \"peace time\"}\n      </p>\n\n      <Chaos\n        onClick={() => {\n          setChaos((i) => !i);\n          kickOffTransition();\n        }}\n      >\n        Start some chaos\n      </Chaos>\n      <main>{children}</main>\n      <Footer />\n      <Circle ref={cricleRef} />\n      <style jsx global>\n        {`\n          .test {\n            position: ${chaos && \"fixed\"};\n            top: ${chaos && \"0\"};\n            left: ${chaos && \"0\"};\n          }\n        `}\n      </style>\n    </>\n  );\n};\n\nexport default DefaultLayout;\n"]},"metadata":{},"sourceType":"module"}